<view class="sc-class cell" capture-bind:tap="{{ripple ? '_addRipple' : ''}}" capture-bind:longpress="{{ripple ? '_longPress' : ''}}" capture-bind:touchend="{{ripple ? '_touchend' : ''}}"><view class="cell-left"><text class="iconfont left-icon"></text><view class="cell-left-container"><view class="cell-header"><slot></slot></view><view wx:if="{{subHeader}}" class="cell-sub-header">{{subHeader}}</view></view></view><view class="cell-right"><view wx:if="{{rightText}}" class="right-text">{{rightText}}</view><text class="iconfont right-icon"></text><sc-switch wx:if="{{switch}}" checked="{{checked}}" name="{{name}}" disabled="{{disabled}}" value="{{value}}" class="sc-switch-class" catch:change="_switchChange"></sc-switch></view><view wx:for="{{rippleList}}" wx:key="rippleId" id="{{item.rippleId}}" style="width:{{item.width}}px;height:{{item.height}}px;left:{{item.left}}px;top:{{item.top}}px" class="sc-ripple {{item.startAnimate ? item.holdAnimate ? 'sc-ripple-animation-slow-hold' :'sc-ripple-animation-slow' : ''}}" bind:animationend="{{item.holdAnimate ? null : '_scbuttonrippleAnimationend'}}"></view></view>